{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "baseUrl": ".",
    "types": ["vite/client"],
    "paths": {
      "~icons/*": ["./node_modules/.unplugin-icons/*"]
    }
  },
  "include": ["src/**/*", "src/env.d.ts"],
  "exclude": ["node_modules", "dist"]
}

